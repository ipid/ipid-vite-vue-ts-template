{
  "name": "ipid-vite-vue-ts-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "prepare": "husky",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:stylelint": "stylelint --fix '**/*.{css,scss,htm,vue,html}'",
    "lint:eslint": "eslint . --fix",
    "lint": "run-p lint:*",
    "do-lint-staged": "lint-staged",
    "format": "prettier --write ./"
  },
  "dependencies": {
    "element-plus": "2.9.4",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "engines": {
    "node": "*",
    "npm": "【【本项目禁止使用 npm，请使用 pnpm】】",
    "yarn": "【【本项目禁止使用 yarn，请使用 pnpm】】",
    "pnpm": "*"
  },
  "devDependencies": {
    "@tsconfig/node22": "22.0.0",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.13.4",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitejs/plugin-vue-jsx": "4.1.1",
    "@vitest/eslint-plugin": "1.1.31",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.4.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "autoprefixer": "10.4.20",
    "eslint": "9.20.1",
    "eslint-plugin-vue": "9.32.0",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "lint-staged": "15.4.3",
    "npm-run-all2": "7.0.2",
    "postcss-html": "1.8.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.5.1",
    "sass": "1.85.0",
    "stylelint": "16.14.1",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-recommended": "15.0.0",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-recommended-vue": "1.6.0",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "unplugin-element-plus": "0.9.1",
    "vite": "6.1.0",
    "vite-plugin-vue-devtools": "7.7.2",
    "vitest": "3.0.5",
    "vue-tsc": "2.2.2"
  }
}
