{
  "name": "ipid-vite-vue-ts-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "prepare": "husky",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:stylelint": "stylelint --fix '**/*.{css,scss,htm,vue,html}'",
    "lint:eslint": "eslint . --fix",
    "lint": "run-p lint:*",
    "do-lint-staged": "lint-staged",
    "format": "prettier --write ./",
    "test": "vitest"
  },
  "dependencies": {
    "element-plus": "2.9.7",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "engines": {
    "node": "*",
    "npm": "【【本项目禁止使用 npm，请使用 pnpm】】",
    "yarn": "【【本项目禁止使用 yarn，请使用 pnpm】】",
    "pnpm": "*"
  },
  "devDependencies": {
    "@tsconfig/node22": "22.0.1",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.14.0",
    "@vitejs/plugin-vue": "5.2.3",
    "@vitejs/plugin-vue-jsx": "4.1.2",
    "@vitest/eslint-plugin": "1.1.39",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.5.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.24.0",
    "eslint-plugin-vue": "10.0.0",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "lint-staged": "15.5.0",
    "npm-run-all2": "7.0.2",
    "postcss-html": "1.8.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.5.3",
    "sass": "1.86.3",
    "stylelint": "16.18.0",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-recommended": "16.0.0",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-recommended-vue": "1.6.0",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "unplugin-element-plus": "0.9.1",
    "vite": "6.2.5",
    "vite-plugin-vue-devtools": "7.7.2",
    "vitest": "3.1.1",
    "vue-tsc": "2.2.8"
  }
}
