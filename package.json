{
  "name": "ipid-vite-vue-ts-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite optimize && vite --host",
    "build": "npm run lint:all && vite build",
    "preview": "vite preview --host",
    "prepare": "husky install",
    "lint:staged": "lint-staged",
    "lint:all": "vue-tsc && eslint --fix . && stylelint --fix '**/*.{scss,vue,html}' && prettier --write ."
  },
  "dependencies": {
    "@types/node": "20.11.17",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "autoprefixer": "10.4.17",
    "element-plus": "2.5.5",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "9.21.1",
    "husky": "9.0.10",
    "lint-staged": "15.2.2",
    "pinia": "2.1.7",
    "prettier": "3.2.5",
    "sass": "1.70.0",
    "stylelint": "16.2.1",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-recommended-scss": "14.0.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "typescript": "5.3.3",
    "unplugin-element-plus": "0.8.0",
    "vite": "5.1.1",
    "vue": "3.4.18",
    "vue-router": "4.2.5",
    "vue-tsc": "1.8.27"
  },
  "engines": {
    "node": "*",
    "npm": "【【本项目禁止使用 npm，请使用 pnpm】】",
    "yarn": "【【本项目禁止使用 yarn，请使用 pnpm】】",
    "pnpm": "*"
  }
}
