{
  "name": "ipid-vite-vue-ts-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite optimize && vite --host",
    "build": "run-p lint:all \"build-only {@}\" --",
    "preview": "vite preview --host",
    "prepare": "husky",
    "build-only": "vite build",
    "lint:staged": "lint-staged",
    "lint:all": "vue-tsc --build --force && eslint --fix . && stylelint --fix '**/*.{scss,vue,html}' && prettier --write ."
  },
  "dependencies": {
    "vue": "3.4.38",
    "vue-router": "4.4.3",
    "element-plus": "2.8.0"
  },
  "engines": {
    "node": "*",
    "npm": "【【本项目禁止使用 npm，请使用 pnpm】】",
    "yarn": "【【本项目禁止使用 yarn，请使用 pnpm】】",
    "pnpm": "*"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "20.1.3",
    "@vue/tsconfig": "0.5.1",
    "@types/node": "22.4.0",
    "@typescript-eslint/eslint-plugin": "8.1.0",
    "@typescript-eslint/parser": "8.1.0",
    "@vitejs/plugin-vue": "5.1.2",
    "@vitejs/plugin-vue-jsx": "4.0.1",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "9.27.0",
    "husky": "9.1.4",
    "lint-staged": "15.2.9",
    "pinia": "2.2.2",
    "prettier": "3.3.3",
    "sass": "1.77.8",
    "stylelint": "16.8.2",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "typescript": "5.5.4",
    "unplugin-element-plus": "0.8.0",
    "vite": "5.4.1",
    "vue-tsc": "2.0.29",
    "npm-run-all2": "6.2.2"
  }
}
