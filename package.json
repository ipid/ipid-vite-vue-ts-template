{
  "name": "ipid-vite-vue-ts-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run lint:all && vite build",
    "preview": "vite preview --host",
    "prepare": "husky install",
    "lint:staged": "lint-staged",
    "lint:all": "vue-tsc && eslint --fix . && stylelint --fix '**/*.{scss,vue,html}' && prettier --write ."
  },
  "dependencies": {
    "@types/node": "20.6.2",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "@vitejs/plugin-vue": "4.3.4",
    "@vitejs/plugin-vue-jsx": "3.0.2",
    "element-plus": "2.3.14",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-stylelint": "20.0.0",
    "eslint-plugin-vue": "9.17.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "pinia": "2.1.6",
    "postcss-html": "1.5.0",
    "prettier": "3.0.3",
    "sass": "1.67.0",
    "stylelint": "15.10.3",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-recommended-scss": "13.0.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "typescript": "5.2.2",
    "unplugin-element-plus": "0.8.0",
    "vite": "4.4.9",
    "vue": "3.3.4",
    "vue-router": "4.2.4",
    "vue-tsc": "1.8.11"
  },
  "engines": {
    "node": "20.6.1",
    "npm": "【【本项目禁止使用 npm，请使用 pnpm】】",
    "yarn": "【【本项目禁止使用 yarn，请使用 pnpm】】",
    "pnpm": "8.7.5"
  }
}
